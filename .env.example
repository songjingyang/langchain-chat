# Cloudinary文件上传配置
# 请在 https://cloudinary.com 注册账号并获取以下信息
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name_here
NEXT_PUBLIC_CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here

# AI模型API密钥配置
OPENAI_API_KEY=your_openai_api_key_here
GROQ_API_KEY=your_groq_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Next.js配置
NEXT_PUBLIC_APP_NAME=LangChain Chat
NEXT_PUBLIC_APP_VERSION=1.0.0

# 可选配置
NEXT_PUBLIC_DEFAULT_MODEL=openai
NEXT_PUBLIC_MAX_TOKENS=2048
NEXT_PUBLIC_TEMPERATURE=0.7
